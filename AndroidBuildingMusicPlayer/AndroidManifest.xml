<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="it.duccius.musicplayer"
    android:versionCode="3"
    android:versionName="1.3" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <uses-sdk android:minSdkVersion="9" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name="it.duccius.musicplayer.SenaVetus"
            android:configChanges="keyboardHidden|orientation"
            android:label="@string/app_name" >
        </activity>
        <activity android:name="it.duccius.musicplayer.PlayListActivity" >
        </activity>
        <activity
            android:name="it.duccius.download.ListViewMultipleSelectionActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="it.duccius.download.MainActivity"
            android:label="@string/app_name" >            
        </activity>
        <activity
            android:name="it.duccius.download.ResultActivity"
            android:label="@string/result_activity" >
        </activity>
        <activity android:name="it.duccius.musicplayer.AudioPlayerActivity" >
        </activity>
        <activity
            android:name="it.duccius.musicplayer.WebviewActivity"
            android:label="@string/title_activity_webview" >
        </activity>
        <activity
            android:name="it.duccius.musicplayer.GeoActivity"
            android:label="@string/title_activity_geo" >
        </activity>
        <activity
            android:name="it.duccius.musicplayer.Main_Activity"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name="it.duccius.musicplayer.ListItemActivity"
            android:label="@string/title_activity_list_item" >
        </activity>
    </application>

</manifest> <!-- AndroidBuildingMusicPlayerActivity -->


